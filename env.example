# Токен Telegram бота (получить у @BotFather)
BOT_TOKEN=1234567890:AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA

# ID администраторов бота (через запятую)
BOT_ADMINS=123456789,987654321

# Уровень логгирования (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Максимальный размер загружаемого файла в МБ
MAX_FILE_SIZE=200

# URL базы данных (по умолчанию SQLite)
DATABASE_URL=sqlite:///data/bot.db 